<tr class="odd gradeX center">
  <td><%= index_for_list @shippings, shipping_counter %></td>
  <td><%= shipping.title %></td>
  <td><%= number_to_currency(shipping.price, unit: t("currency"),
    separator: ",") %></td>
  <td class="center">
    <i class="fa fa-pencil fa-fw"></i>
    <%= link_to t(".edit"), edit_admin_shipping_path(shipping) %>
  </td>
  <td class="center">
    <i class="fa fa-trash-o  fa-fw"></i>
    <%= link_to t(".delete"), admin_shipping_path(shipping),
      method: :delete, data: {confirm: t(".you_sure")} %>
  </td>
</tr>
